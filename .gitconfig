[user]
    name =
    email =
[core]
    editor = nano
    excludesfile = ~/.gitignore
    filemode = false
    ignorecase = true
    pager = less -R
[color]
    ui = true
[alias]
    a = add
    aa = add --all
    b = branch
    c = commit -v -m
    ca = commit -v -a -m
    co = checkout
    d = diff
    dc = diff --cached
    dt = difftool
    lg = log --graph --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold white)â€” %an%C(reset)%C(bold yellow)%d%C(reset)' --abbrev-commit --date=relative
    l = pull
    p = push
    pullsubs = submodule foreach git pull origin master
    s = status
    today = shortlog --since=midnight
    uncommit = reset --soft HEAD^
    unamend = reset --soft HEAD@{1}
    who = shortlog -s -n --no-merges
    yesterday = shortlog --since=yesterday
[branch "master"]
    rebase = true
[difftool "sourcetree"]
    cmd = opendiff \"$LOCAL\" \"$REMOTE\"
    path =
[mergetool "sourcetree"]
    cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
    trustExitCode = true